@using RazorEngine.Mailing.Library.Models

@inherits RazorEngineCore.RazorEngineTemplateBase<RazorEngine.Mailing.Library.Models.AuditProfileViewModel>

@{
    Layout = "layout";
}


<!-- Header -->
<table align="center" width="100%" cellspacing="0" cellpadding="0">
    <tbody>
        <tr>
            <td style="text-align: center;">
                <p class="mail-title">Er is een wachtwoordwijziging doorgevoerd voor je account</p>
            </td>
        </tr>
    </tbody>
</table>

<!-- Action -->
<p>
    <b><span class="blurrable">@Model.AuditDescription</span></b>
</p>

<p>Ga voor meer gedetailleerde informatie naar je <a href="@Model.PortalAuditUrl" target="_blank">account</a>.</p>
