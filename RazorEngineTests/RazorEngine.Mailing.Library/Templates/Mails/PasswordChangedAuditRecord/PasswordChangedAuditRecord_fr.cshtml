@using RazorEngine.Mailing.Library.Models

@inherits RazorEngineCore.RazorEngineTemplateBase<RazorEngine.Mailing.Library.Models.AuditProfileViewModel>

@{
    Layout = "layout";
}

<!-- Header -->
<table align="center" width="100%" cellspacing="0" cellpadding="0">
    <tbody>
        <tr>
            <td style="text-align: center;">
                <p class="mail-title">Votre compte a fait l'objet d'un changement de mot de passe</p>
            </td>
        </tr>
    </tbody>
</table>

<!-- Action -->
<p>
    <b><span class="blurrable">@Model.AuditDescription</span></b>
</p>

<p>Pour obtenir des informations plus détaillées, consultez le portail de votre <a href="@Model.PortalAuditUrl" target="_blank">compte</a>.</p>
